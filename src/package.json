		"@vitest/browser": "^3.0.7",
		"@vitest/coverage-v8": "^3.0.7",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"mdsvex": "^0.12.3",
		"playwright": "^1.50.1",
